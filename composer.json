{
    "name": "lawoole/lawoole",
    "description": "The Lawoole framework",
    "keywords": ["framework", "swoole", "laravel", "lawoole"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "lawoole/framework": "0.5.*",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "lawoole/swoole": "dev-master",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
